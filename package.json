{
  "name": "payment-mf",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "NODE_OPTIONS='--max-http-header-size=24576' nuxt-ts start",
    "generate": "nuxt-ts generate",
    "test": "jest",
    "generate:snap": "find ./test/components -name '*.ts.snap' -type f -delete | yarn jest --testPathIgnorePatterns=test/contract",
    "test:unit": "jest --testPathIgnorePatterns=test/contract --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "badge": "jest-coverage-badges",
    "test:contract": "jest --testPathPattern=test/contract",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . --fix",
    "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
    "test:acceptance": "npx wdio test/acceptance/wdio.conf.js",
    "all": "yarn run build && yarn run lint && yarn run test:unit && yarn run test:contract && yarn run test:acceptance",
    "audit": "improved-yarn-audit --ignore-dev-deps --min-severity moderate -e ${NPM_EXCLUDED_VULNERABILITIES}"
  },
  "dependencies": {
    "@adyen/adyen-web": "^4.7.0",
    "@nuxt/typescript-runtime": "^1.0.0",
    "axios": "^0.21.1",
    "card": "2.5.0",
    "credit-card-type": "^9.1.0",
    "luhn": "^2.4.1",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.15.4",
    "vee-validate": "^4.0.6",
    "whatwg-fetch": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@femessage/nuxt-micro-frontend": "^1.6.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@nuxt/types": "^2.14.0",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/tailwindcss": "^4.0.3",
    "@nuxtjs/vuetify": "^1.11.2",
    "@pact-foundation/pact": "^9.11.1",
    "@types/cucumber": "^6.0.1",
    "@types/jest": "^26.0.14",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/test-utils": "^1.0.3",
    "@wdio/cli": "^6.0.8",
    "@wdio/config": "^6.0.8",
    "@wdio/cucumber-framework": "^6.0.8",
    "@wdio/local-runner": "^6.0.8",
    "@wdio/selenium-standalone-service": "^6.0.8",
    "@wdio/spec-reporter": "^6.0.8",
    "@wdio/static-server-service": "^6.0.8",
    "@wdio/sync": "^6.0.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "chromedriver": "^87.0.5",
    "debug-mode": "^2.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-wdio": "^6.0.8",
    "flush-promises": "^1.0.2",
    "http-server": "^13.0.2",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "improved-yarn-audit": "^2.3.3",
    "jest": "^24.1.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-junit": "^12.2.0",
    "jest-pact": "^0.5.4",
    "npm-run-all": "^4.1.5",
    "nuxt-property-decorator": "^2.8.8",
    "postcss": "^8.2.9",
    "tailwindcss-rtl": "^0.7.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.4",
    "vue-jest": "^4.0.0-0",
    "vue-unleash": "^1.1.0",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.0.8"
  }
}
